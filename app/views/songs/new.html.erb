<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist %>
  <%= f.text_field :artist %>
  <%= f.collection_select :genre, Genre.all, :id, :name %>
  <%= f.text_field :song_notes_1 %>
  <%= f.text_field :song_notes_2 %>
  <%= f.text_field :song_notes_3 %>
  <br />

  <%= f.submit %>
<% end %>
